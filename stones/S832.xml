      <div type="inscription" xml:lang="en" decls="#f_S #c_3 #m_RS #t_P">
        <ab>SACRED TO THE MEMORY</ab>
        <ab>OF</ab>
        <ab>LUCY ELDRIDGE LEE</ab>
        <ab/>
        <ab>WIDOW OF WILLIAM LEE OF NEW=YORK</ab>
      </div>
      <div type="inscription" xml:lang="en" decls="#f_E #c_3 #m_RS #t_P">
        <ab>AND HER CHILDREN SHALL RISE</ab>
        <ab>UP AND CALL HER BLESSED</ab>
      </div>
      <div type="inscription" xml:lang="en" decls="#f_N #c_3 #m_RS #t_P">
        <ab>BORN IN UTICA NEW=YORK IN 1832</ab>
        <ab>DIED IN ROME 7TH JANUAR.1918</ab>
      </div>
      <div type="inscription" xml:lang="en" decls="#f_W #c_3 #m_RS #t_P">
        <ab>WELL DONE THOU GOOD AND FAITHFUL</ab>
        <ab>SERVANT ENTER THOU INTO THE JOY OF THY LORD</ab>
      </div>
