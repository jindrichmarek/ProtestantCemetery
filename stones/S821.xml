    <body>
      <div type="inscription"  xml:lang="en" decls="#f_T #c_3 #m_I #t_P">
        <ab>ROBERT G. STANNARD</ab>
        <ab>DIED THE 17TH DEC 1945</ab>
        <ab>IN HIS EIGHTYNINETH YEAR</ab>
        <ab/>
        <ab>HE THE BEST</ab>
        <ab>AND HE THE NOBLEST</ab>
        <ab>HE SO GENTLE</ab>
        <ab>AND SO KIND</ab>
      </div>
      <div type="inscription"  xml:lang="en" decls="#f_T #c_3 #m_I #t_S">
        <ab>EVA A. STANNARD</ab>
        <ab>BORN LOESSIN</ab>
        <ab>DIED THE 22ND AUG. 1958</ab>
        <ab>IN HER EIGHTYFOURTH YEAR</ab>
        <ab/>
        <ab>HIS BELOVED</ab>
        <ab>AND FAITHFUL WIFE</ab>
        <ab>RESTS NEAR HIM</ab>
      </div>
    </body>
